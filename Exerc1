int qtdLinOriginal = 3;
int qtdColOrginal = 2;

int[][] matrizOriginal = new int[qtdLinOriginal][qtdColOrginal];
int[][] matrizTransposta = new int[qtdColOriginal][qtdLinOriginal];

matrizOriginal[0][0] = 5;
matrizOriginal[0][1] = 0;
matrizOriginal[1][0] = -1;
matrizOriginal[1][1] = 2;
matrizOriginal[2][0] = 0;
matrizOriginal[2][1] = 6;


for(int i = 0; i < qtdColOriginal; i++) {
  for(int j = 0; j < qtdLinOrinal; j++){
    matrizTransposta[i][j] = matrizOriginal[j][i];
    }
}
